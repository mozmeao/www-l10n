error-page-mozilla = { -brand-name-mozilla }
error-page-firefox = { -brand-name-firefox }
error-page-error-page-internal-server-error = 500: የውስጥ አገልጋይ ስህተት
error-page-something-went-wrong = የሆነ ስህተት ተከስቷል
error-page-its-probably-just-a-server-error = ምናልባት የአገልጋይ ስህተት ብቻ ነው እና ለማስተካከል እየሰራን ነው።
# Variables:
#   $firefox (url) - link to https://firefox.com/
#   $mozilla (url) - link to https://www.mozilla.org/
error-page-you-can-also-try-refreshing = እንዲሁም ይህን ገጽ ለማደስ መሞከር ወይም ወደ <a href={ $firefox }>firefox.com</a> ወይም <a href={ $mozilla }>mozilla.org</a> መሄድ ይችላሉ።
