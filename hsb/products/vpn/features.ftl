    { $devices ->
        [one] Zwjazajće hač do { $devices } grata
        [two] Zwjazajće hač do { $devices } gratow
        [few] Zwjazajće hač do { $devices } gratow
       *[other] Zwjazajće hač do { $devices } gratow
    }
